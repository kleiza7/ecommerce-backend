  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only ./index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "check:ts": "tsc --noEmit",
    "seed": "ts-node ./seed/index.ts",
